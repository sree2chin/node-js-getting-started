{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "redirects": [
    {
      "source": "/blog/:path*",
      "destination": "https://medium.com/@srikanthb/:path*",
      "permanent": false
    }
  ]
}